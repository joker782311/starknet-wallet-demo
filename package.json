{
  "name": "node-starknet",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Starter Node.js template",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --ext js,mjs,cjs --exec node src/index.js",
    "lint": "eslint .",
    "test": "node --test",
    "test:account": "node --test test/create_account.test.js",
    "devnet": "starknet-devnet",
    "test:jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:jest:account": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/create_account.jest.test.js -t '在主网创建/激活 OpenZeppelin 账户（若已激活则校验）'"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "starknet": "^8.6.0",
    "starknet-devnet": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.3",
    "jest": "^29.7.0"
  }
}